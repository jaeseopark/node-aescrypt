{
  "name": "node-aescrypt",
  "version": "0.1.0",
  "description": "A node implementation of the AES Crypt <https://www.aescrypt.com/> file encryption format.",
  "author": "Judah Anthony",
  "repository": "github:judahtanthony/node-aescrypt",
  "main": "dist/index.js",
  "bin": {
    "aescrypt": "bin/aescrypt"
  },
  "directories": {
    "src": "src",
    "dist": "dist",
    "bin": "bin"
  },
  "scripts": {
    "build": "webpack",
    "prebuild-cli": "npm run build",
    "build-cli": "echo '#!/usr/bin/env node' > bin/aescrypt && cat dist/cli.js >> bin/aescrypt && chmod +x bin/aescrypt",
    "pretest": "npm run build",
    "test": "mocha dist/**/*.spec.js",
    "lint": "eslint src --ext ts",
    "aescrypt": "aescrypt"
  },
  "keywords": [
    "AES",
    "Crypt",
    "aescrypt",
    "encryption",
    "encrypt",
    "decrytion",
    "decrypt"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.1.2",
    "command-line-args": "^5.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-promise": "^3.7.0",
    "mocha": "^5.2.0",
    "source-map-loader": "^0.2.3",
    "ts-loader": "^4.3.0",
    "typescript": "^2.8.3",
    "typescript-eslint-parser": "^15.0.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  },
  "license": "MIT"
}
